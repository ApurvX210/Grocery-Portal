1/auth/update-profile`, { name, email, address, phone })\r\n      if(data?.error){\r\n        toast.error(data?.error);\r\n      }else{\r\n        setAuth({...auth,user:data?.updateUser})\r\n        let ls=localStorage.getItem(\"auth\");\r\n        ls=JSON.parse(ls);\r\n        ls.user=data.updateUser\r\n        localStorage.setItem(\"auth\",JSON.stringify(ls));\r\n        toast.success(\"Profile Updated Successfully\")\r\n      }\r\n    } catch (error) {\r\n      console.log(error);\r\n      toast.error('Something went wrong');\r\n    }\r\n  }\r\n  //getUserData\r\n  useEffect(() => {\r\n    const { name, email, address, phone } = auth?.user;\r\n    const n = name.split(\" \");\r\n    setFname(n[0]);\r\n    setLname(n[1]);\r\n    setAddress(address);\r\n    setPhone(phone);\r\n    setEmail(email);\r\n  }, [auth?.user]);\r\n  return (\r\n    <Layout title=\"Dashboard - Profile\">\r\n      <div className='container-fluid m-4 p-4'>\r\n        <div className='row'>\r\n          <div className='col-md-3'>\r\n            <UserMenu />\r\n          </div>\r\n          <div className='col-md-9'>\r\n            <div className='form-container' style={{ backgroundImage: \"none\" }}>\r\n              <form className=\"row g-3\" onSubmit={handleSubmit}>\r\n                <div className=\"title\" style={{ textAlign: 'center' }}>\r\n                  <h1>User Profile</h1>\r\n                </div>\r\n                <div className=\"col-md-6\">\r\n                  <label htmlFor=\"inputFName\" className=\"form-label\">First Name :</label>\r\n                  <input type=\"text\"  onChange={(event) => {\r\n                    setFname(event.target.value);\r\n                  }} value={Fname} className=\"form-control\" id=\"inputEmail4\" placeholder=\"First Name\" />\r\n                </div>\r\n                <div className=\"col-md-6\">\r\n                  <label htmlFor=\"inputLName\" className=\"form-label\">Last Name :</label>\r\n                  <input type=\"text\" onC